{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "temperature": {
      "type": "string",
      "pattern": "^\\d+(\\.\\d+)?$",
      "description": "temperature as a string representing units of Celcius"
    },
    "timestamp": {
      "type": "string",
      "format": "date-time",
      "description": "ISO 8601 timestamp"
    }
  },
  "required": ["temperature", "timestamp"],
  "additionalProperties": false
}
